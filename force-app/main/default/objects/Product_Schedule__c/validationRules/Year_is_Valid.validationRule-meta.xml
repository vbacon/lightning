<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Year_is_Valid</fullName>
    <active>true</active>
    <errorConditionFormula>OR(NOT(ISNUMBER(Fiscal_Year__c)), VALUE(Fiscal_Year__c)&lt;2010 )</errorConditionFormula>
    <errorDisplayField>Fiscal_Year__c</errorDisplayField>
    <errorMessage>Enter a 4 digit year > 2010</errorMessage>
</ValidationRule>
